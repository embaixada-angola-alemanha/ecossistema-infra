{
  "uid": "eco-service-overview",
  "title": "Ecossistema — Service Overview",
  "description": "High-level overview of all Ecossistema Digital backend services: SGC, SI, WN, and GPJ. Shows service health, uptime, request rates, and error rates.",
  "tags": ["ecossistema", "overview", "spring-boot"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "fiscalYearStartMonth": 0,
  "liveNow": false,
  "editable": true,
  "graphTooltip": 1,
  "templating": {
    "list": [
      {
        "name": "env",
        "type": "custom",
        "label": "Environment",
        "current": {
          "selected": true,
          "text": "staging",
          "value": "staging"
        },
        "options": [
          { "selected": true, "text": "staging", "value": "staging" },
          { "selected": false, "text": "production", "value": "production" }
        ],
        "query": "staging,production",
        "hide": 0
      },
      {
        "name": "service",
        "type": "custom",
        "label": "Service",
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        },
        "options": [
          { "selected": true, "text": "All", "value": "$__all" },
          { "selected": false, "text": "sgc-backend", "value": "sgc-backend" },
          { "selected": false, "text": "si-backend", "value": "si-backend" },
          { "selected": false, "text": "wn-backend", "value": "wn-backend" },
          { "selected": false, "text": "gpj-backend", "value": "gpj-backend" }
        ],
        "query": "sgc-backend,si-backend,wn-backend,gpj-backend",
        "includeAll": true,
        "multi": true,
        "hide": 0
      }
    ]
  },
  "panels": [
    {
      "type": "row",
      "title": "Service Health Status",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 100,
      "panels": []
    },
    {
      "id": 1,
      "type": "stat",
      "title": "SGC Backend",
      "description": "Sistema de Gestao Consular — health status",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        {
          "refId": "A",
          "expr": "up{service=\"sgc-backend\", env=\"$env\"}",
          "legendFormat": "SGC",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "0": { "text": "DOWN", "color": "red", "index": 0 } } },
            { "type": "value", "options": { "1": { "text": "UP", "color": "green", "index": 1 } } }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          },
          "color": { "mode": "thresholds" },
          "noValue": "N/A"
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "auto",
        "wideLayout": true,
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto"
      }
    },
