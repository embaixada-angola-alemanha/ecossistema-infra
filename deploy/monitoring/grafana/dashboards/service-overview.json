        },
        {
          "refId": "D",
          "expr": "sum(rate(http_server_requests_seconds_count{service=\"gpj-backend\", env=\"$env\", status=~\"5..\"}[5m])) or vector(0)",
          "legendFormat": "GPJ 5xx"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 25,
            "gradientMode": "scheme",
            "spanNulls": false,
            "showPoints": "auto",
            "pointSize": 5,
            "stacking": { "mode": "none", "group": "A" },
            "axisPlacement": "auto",
            "axisLabel": "",
            "scaleDistribution": { "type": "linear" },
            "hideFrom": { "tooltip": false, "viz": false, "legend": false },
            "thresholdsStyle": { "mode": "area" }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 0.5 },
              { "color": "red", "value": 2 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max", "lastNotNull"] }
      }
    }
