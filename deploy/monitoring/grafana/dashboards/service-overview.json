    {
      "type": "row",
      "title": "Uptime (Last 24h)",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "id": 101,
      "panels": []
    },
    {
      "id": 5,
      "type": "gauge",
      "title": "SGC Uptime (24h)",
      "gridPos": { "h": 5, "w": 6, "x": 0, "y": 6 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        {
          "refId": "A",
          "expr": "avg_over_time(up{service=\"sgc-backend\", env=\"$env\"}[24h]) * 100",
          "legendFormat": "SGC",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 90 },
              { "color": "yellow", "value": 95 },
              { "color": "green", "value": 99 }
            ]
          },
          "color": { "mode": "thresholds" },
          "decimals": 2
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      }
    },
