    {
      "type": "row",
      "title": "Request Rate",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 11 },
      "id": 102,
      "panels": []
    },
    {
      "id": 9,
      "type": "timeseries",
      "title": "HTTP Request Rate (req/s)",
      "description": "Rate of incoming HTTP requests per service over a 5-minute sliding window",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 12 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(http_server_requests_seconds_count{service=\"sgc-backend\", env=\"$env\"}[5m]))",
          "legendFormat": "SGC"
        },
        {
          "refId": "B",
          "expr": "sum(rate(http_server_requests_seconds_count{service=\"si-backend\", env=\"$env\"}[5m]))",
          "legendFormat": "SI"
        },
        {
          "refId": "C",
          "expr": "sum(rate(http_server_requests_seconds_count{service=\"wn-backend\", env=\"$env\"}[5m]))",
          "legendFormat": "WN"
        },
        {
          "refId": "D",
          "expr": "sum(rate(http_server_requests_seconds_count{service=\"gpj-backend\", env=\"$env\"}[5m]))",
          "legendFormat": "GPJ"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 15,
            "gradientMode": "scheme",
            "spanNulls": false,
            "showPoints": "auto",
            "pointSize": 5,
            "stacking": { "mode": "none", "group": "A" },
            "axisPlacement": "auto",
            "axisLabel": "",
            "scaleDistribution": { "type": "linear" },
            "hideFrom": { "tooltip": false, "viz": false, "legend": false },
            "thresholdsStyle": { "mode": "off" }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max", "lastNotNull"] }
      }
    },
    {
      "type": "row",
      "title": "Error Rate",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 20 },
      "id": 103,
      "panels": []
    },
    {
      "id": 10,
      "type": "timeseries",
      "title": "HTTP 5xx Error Rate (errors/s)",
      "description": "Rate of server-side errors (HTTP 5xx) per service over a 5-minute sliding window",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 21 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(http_server_requests_seconds_count{service=\"sgc-backend\", env=\"$env\", status=~\"5..\"}[5m])) or vector(0)",
          "legendFormat": "SGC 5xx"
        },
        {
          "refId": "B",
          "expr": "sum(rate(http_server_requests_seconds_count{service=\"si-backend\", env=\"$env\", status=~\"5..\"}[5m])) or vector(0)",
          "legendFormat": "SI 5xx"
        },
        {
          "refId": "C",
          "expr": "sum(rate(http_server_requests_seconds_count{service=\"wn-backend\", env=\"$env\", status=~\"5..\"}[5m])) or vector(0)",
